        {!ocaml-option-32bit:installed & !ocaml-option-musl:installed &
         (os = "openbsd" | os = "macos")}
      "ASPP=musl-gcc -c"
        {ocaml-option-musl:installed & os-distribution != "alpine"}
      "LIBS=-static" {ocaml-option-static:installed}
    ]
    [make "-j%{jobs}%"]
